/*
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    Nextflow config file for running mouse test
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
*/

params {
    // Test parameters
    organism            = 'mus_musculus'
    subsample_ref       = 500
    subsample_query     = 100
    ref_split           = 'dataset_id'
    cutoff              = 0
    normalization_method = 'SCT'
    subset_type         = 'sample'
    batch_correct       = true
    use_gap             = false
    remove_unknown      = true

    // Output
    outdir_prefix       = 'mmus_new'
}
