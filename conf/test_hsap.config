/*
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    Nextflow config file for running human test
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
*/

params {
    // Test parameters
    organism            = 'homo_sapiens'
    subsample_ref       = 500
    subsample_query     = 100
    ref_split           = 'dataset_id'
    cutoff              = 0
    normalization_method = 'SCT'
    subset_type         = 'sample'
    batch_correct       = true
    use_gap             = false

    // Output
    outdir_prefix       = '2024-07-01/homo_sapiens_new/100/dataset_id/SCT/gap_false'
}
